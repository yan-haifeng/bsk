<view class="body"><view class="login_form"><view class="form_input"><input type="text" name="username" id="username" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['','username','$event',[]]]]]]}}" value="{{username}}" bindinput="__e"/></view><view class="form_input v_code_input"><input type="text" name="verificationcode" id="verificationcode" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','verificationcode','$event',[]]]]]]}}" value="{{verificationcode}}" bindinput="__e"/><vcodebtn vue-id="1" phone="{{username}}" flag="1" bind:__l="__l"></vcodebtn></view><view class="form_input"><input type="password" name="password" id="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="form_input"><input type="password" name="aginpassword" id="aginpassword" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['','againpassword','$event',[]]]]]]}}" value="{{againpassword}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="login_submit" bindtap="__e">注册</view></view></view>