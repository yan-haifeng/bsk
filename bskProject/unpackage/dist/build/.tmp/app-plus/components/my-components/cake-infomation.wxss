
.information_box {
	width: 100%;
	height: 100%;
	background-color: #FFFFFF;
	border-top: #25E0E4 5px solid;
	/* position: absolute;
	bottom: 0upx;
	left: 3%; */
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	padding: 40rpx 30rpx;
}
.cake_box {
	width: 100%;
	overflow: hidden;
	position: relative;
}
.cake_box .cake_title_box {
	float: left;
	height: 100%;
	font-size: 28rpx;
}
.cake_box .cake_title_box .cake_title {
	font-size: 36rpx;
	color: #000000;
	font-weight: bold;
}
.cake_box .cake_title_box .cake_title_english {
	color: #9e9e9e;
}
.cake_box .cake_price {
	position: absolute;
	color: #FF1850;
	font-size: 44rpx;
	font-weight: bold;
	right: 0rpx;
}
.specificat {
	margin-top: 30rpx;
	overflow: hidden;
	height: 60rpx;
}
.form_title {
	display: block;
	width: 30%;
	font-size: 28rpx;
	float: left;
	height: 100%;
	line-height: 50rpx;
}
.specificat .specificat_operat {
	float: left;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	width: 70%;
}
.specificat .specificat_operat .specificat_btn {
	width: 90rpx;
	height: 50rpx;
	font-size: 28rpx;
	border: #CCCCCC 1px solid;
	color: #CCCCCC;
	border-radius: 10rpx;
	text-align: center;
	line-height: 50rpx;
	margin-right: 10rpx;
}
.specificat .specificat_operat .btn_click {
	color: #FFFFFF !important;
	border: #25E0E4 1px solid !important;
	background: #25E0E4 !important;
}
.cake_num {
	margin-top: 30rpx;
}
.cake_choice {
	width: 170rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
}
.cake_choice ._img {
	display: inline-block;
	width: 50rpx;
	height: 50rpx;
}
.cake_sweet_box {
	margin-top: 30rpx;
	overflow: hidden;
	height: 60rpx;
}
.information_title {
	display: block;
	width: 15%;
	font-size: 28rpx;
	float: left;
	height: 100%;
	line-height: 50rpx;
}
.cake_sweet_box .cake_sweet {
	width: 80%;
	height: 100%;
	float: left;
}
.cake_sweet_box .cake_sweet ._span {
	display: block;
	width: 50rpx;
	height: 50rpx;
	margin-right: 2rpx;
	background-image: url(data:image/png;base64,);
	background-size: 100% 100%;
	background-repeat: no-repeat;
	float: left;
}
.cake_sweet_box .cake_sweet .sweet {
	background-image: url(data:image/png;base64,);
}
.cake_infomation {
	width: 100%;
	overflow: hidden;
}
.cake_infomation .infomation_item {
	width: 50%;
	float: left;
}
.cake_infomation .infomation_item view {
	font-size: 28rpx;
	margin-top: 20rpx;
}
.cake_details {
	display: block;
	font-size: 28rpx;
	margin-top: 40rpx;
	color: #9e9e9e;
}
.join_cakecar_btn {
	widows: 100%;
	height: 100rpx;
	background: -webkit-gradient(linear, left top, right top, from(#FE7196), to(#FF1850));
	background: -o-linear-gradient(left, #FE7196, #FF1850);
	background: linear-gradient(to right, #FE7196, #FF1850);
	border-radius: 100rpx;
	color: #FFFFFF;
	text-align: center;
	line-height: 100rpx;
	margin-top: 40rpx;
}

